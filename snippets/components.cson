  'Circle Avatar Title':
    'prefix': 'circle-avatar-title'
    'body': """
    <header class="tc pv4 pv5-ns">
      <img src="${1:http://www.fillmurray.com/200/200}" class="br-100 pa1 ba b--black-10 h3 w3" alt="avatar">
      <h1 class="f5 f4-ns fw6 mid-gray">${2:Bill Murray}</h1>
      <h2 class="f6 gray fw2 ttu tracked">${3:Charleston, SC}</h2>
    </header>
    """

  'Fixed Semi Transparent':
    'prefix': 'fixed-semi-transparent'
    'body': """
    <header class="bg-black-90 fixed w-100 ph3 pv3 pv4-ns ph4-m ph5-l">
      <nav class="f6 fw6 ttu tracked">
        <a class="link dim white dib mr3" href="#" title="$1">$2</a>
        <a class="link dim white dib mr3" href="#" title="$2">$3</a>
        <a class="link dim white dib mr3" href="#" title="$4">$5</a>
        <a class="link dim white dib" href="#" title="$6">$7</a>
      </nav>
    </header>
    """

  'Rounded Avatar Title Subtitle':
