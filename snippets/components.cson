        <a href="${5:/terms/}"    title="${6:Terms}" class="f6 dib ph2 link mid-gray dim">${7:Terms of Use}</a>
        <a href="${8:/privacy/}"  title="${9:Privacy}" class="f6 dib ph2 link mid-gray dim">${10:Privacy}</a>
      </div>
    </footer>
    """

